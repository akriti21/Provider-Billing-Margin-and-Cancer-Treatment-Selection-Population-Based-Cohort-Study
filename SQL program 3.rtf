\par \hich\af4\dbch\af31505\loch\f4  (case when\hich\af4\dbch\af31505\loch\f4  dx in ('412',       'I252') then 1 else 0 end) as history_mi,  /* History myocardial infarction */
\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('39891','40201','40211','40291','40401','40403','40411','40413','40491','40493','428') or ('4254'<=dx and dx <='4259')        or 
\par \hich\af4\dbch\af31505\loch\f4 dx in ('I099','I110','I130','I132','I255','I420','I43','I50','P290') or ('I425'<=dx and dx<='I429') then 1 else 0 end) as chf,  /* Congestive heart failure */

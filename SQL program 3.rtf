\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('0930','440','441','4471','5571','5579','V434') or ('4431'<=dx and dx <='4439')  \hich\af4\dbch\af31505\loch\f4     or 
\par \hich\af4\dbch\af31505\loch\f4 dx in ('I70','I71','I731','I738','I739','I771','I790','I792','K551','K558','K559','Z958','Z959') then 1 else 0 end) as pvd,  /* Peripheral vascular disease */
\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('36234') or ('430'<=dx and dx <='438')     or 
\par \hich\af4\dbch\af31505\loch\f4 dx in ('G45','G46','H340') or ('I60'<=dx and dx <='I69') then 1 else 0 end) as cvd,  /* Cerebrovascular disease */
\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('4168','4169','5064','5081','5088') or ('490'<=dx and dx <='505')    or 
\par \hich\af4\dbch\af31505\loch\f4 dx in ('I278','I279','J684','J701','J703') or ('J40'<=dx and dx <='J47') o\hich\af4\dbch\af31505\loch\f4 r ('J60'<=dx and dx <='J67') then 1 else 0 end) as copd,  /* Chronic pulmonary disease */
\par \hich\af4\dbch\af31505\loch\f4  (case when dx in ('290','2941','3312')     or dx in ('F051','G30','G311') or ('F00'<=dx and dx <='F03')  then 1 else 0 end) as dementia,  /* Dementia */
\par \hich\af4\dbch\af31505\loch\f4 
 (case when dx in ('3341','342','343','3449') or ('3440'<=dx and dx <='3446')     or dx in ('G041','G114','G801','G802','G81','G82','G839') or ('G830'<=dx and dx <='G834') then 1 else 0 end) as paralysis,  /* Hemiplegia or paraplegia */
\par 
\par 
\par \hich\af4\dbch\af31505\loch\f4 (case when ('2500'<=dx and dx <='2503') or dx in('2508','2509')                or 
\par \hich\af4\dbch\af31505\loch\f4 
dx in ('E100','E101','E106','E108','E109','E110','E111','E116','E118','E119','E120','E121','E126',  'E128','E129','E130','E131','E136','E138','E139','E140','E141','E146','E148','E149') then 1 else 0 end) as diabetes,  /* Diabetes w/out chronic complicatio
\hich\af4\dbch\af31505\loch\f4 ns */
\par \hich\af4\dbch\af31505\loch\f4 (case when ('2504'<=dx and dx <='2507')   OR
\par \hich\af4\dbch\af31505\loch\f4 dx in ('E107','E117','E127','E137','E147') or ('E102'<=dx and dx<='E105') or ('E112'<=dx and dx <='E115') or  ('E122'<=dx a\hich\af4\dbch\af31505\loch\f4 
nd dx <='E125') or ('E132'<=dx and dx <='E135') or ('E142'<=dx and dx <='E145')  then 1 else 0 end) as diabetes_comp,  /* Diabetes w/chronic complications */
\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('40301','40311','40391','40402','40403','40412','40413','40492','40493','582','585','586','5880','V420','V451','V56') or  ('5830'<=dx and dx <='5837')   OR
\par \hich\af4\dbch\af31505\loch\f4 dx in ('I120','I131','N18','N19','N250','Z940','Z992') or ('N032'<=dx and dx <='N037') or ('N052'<=dx and dx<='N057') or ('Z490'<=dx and dx <='Z492') then 1 else 0 end) as renal_di\hich\af4\dbch\af31505\loch\f4 
sease,  /* Renal disease */
\par \hich\af4\dbch\af31505\loch\f4 (case when dx in ('07022','07023','07032','07033','07044','07054','0706','0709','570','571','5733','5734','5738','5739','V427')    OR 
\par \hich\af4\dbch\af31505\loch\f4 
dx in ('B18','K709','K717','K73','K74','K760','K768','K769','Z944') or ('K700'<=dx and dx <='K703') or   ('K713'<=dx and dx <='K715') or ('K762'<=dx and dx <='K764') then 1 else 0 end) as mild_liver_disease,  /* Mild liver disease */
\par 
\par 
\par \hich\af4\dbch\af31505\loch\f4  (case when ('4560'<=dx and dx <='4562') or ('5722'<=dx and dx <='5728')       or dx in ('I850','I859','I864','I9\hich\af4\dbch\af31505\loch\f4 
82','K704','K711','K721','K729','K765','K766','K767')  then 1 else 0 end) as liver_disease,  /* Moderate/Severe liver disease */
\par \hich\af4\dbch\af31505\loch\f4  (case when ('531'<=dx and dx <='534')        or ('K25'<=dx and  dx <='K28') then 1 else 0 end) as ulcers,  /* Peptic ulcer disease */
\par \hich\af4\dbch\af31505\loch\f4  (case when dx in ('4465','7148','725') or ('7100'<=dx and dx <='7104') or ('7140'<=dx and dx <='7142')      or 
\par \hich\af4\dbch\af31505\loch\f4 dx in ('M05','M06','M315','M351','M353','M360') or ('M32'<=dx and dx <='M34')  then 1 else 0 end) as rheum_disease,  /* Rheumatic disea\hich\af4\dbch\af31505\loch\f4 se */
\par \hich\af4\dbch\af31505\loch\f4  (case when ('042'<=dx and dx <='044')     or ('B20'<=dx and dx <='B22') or dx in('B24') then 1 else 0 end) as aids  /* AIDS/HIV */
\par 
\par \hich\af4\dbch\af31505\loch\f4   \tab from IN_icdclms
